<Window x:Class="Zvuki.ClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zvuki"
        mc:Ignorable="d"
        Title="ClientWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Button Content="My recordings" Grid.Row="0"/>
            <Button Content="My groups" Grid.Row="1"/>
            <Button Content="My rents" Grid.Row="2"/>
            <Button Content="My invations" Grid.Row="3"/>
            <Button Content="My results" Grid.Row="4"/>
        </Grid>
        <Frame x:Name="clientFrame" NavigationUIVisibility="Hidden" Grid.Column="1"/>


    </Grid>
</Window>
