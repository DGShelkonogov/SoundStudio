        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zvuki"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="AdType" HorizontalAlignment="Left" Margin="10,80,0,0" TextWrapping="Wrap" Text="AdType" VerticalAlignment="Top" Width="120"/>
        <Button Content="Add" Click="Button_Click_Add_Ad" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top" Width="120"/>
        <ListBox Name="AddList" Margin="300,0,0,0">
           
        </ListBox>
    </Grid>
</Window>
